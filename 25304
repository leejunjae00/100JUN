#ifndef _CRT_SECURE_NO_WARNINGS
#define _CRT_SECURE_NO_WARNINGS
#endif

#include <stdio.h>

int main() {
	int x, n, a, b, sum = 0;
	scanf("%d %d", &x, &n);

	for (int i = 0; i < n; ++i) {
		scanf("%d %d", &a, &b);
		sum += a * b;
	}

	if (sum == x) printf("Yes\n");
	else printf("No\n");

	return 0;
}
